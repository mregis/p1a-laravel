{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "askedio/laravel5-soft-cascade": "^5.6",
        "barryvdh/laravel-dompdf": "^0.8.3",
        "canducci/cep": "2.0.*",
        "darkaonline/l5-swagger": "5.6.*",
        "doctrine/dbal": "^2.6",
        "fideloper/proxy": "~4.0",
        "geekcom/validator-docs": "^1.2",
        "intervention/image": "dev-master",
        "iugu/iugu": "^1.0",
        "laravel/framework": "5.6.*",
        "laravel/passport": "^5.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "~5.0",
        "laravellegends/pt-br-validator": "5.1.*",
        "lcobucci/jwt": "^3.2",
        "maatwebsite/excel": "^3.1",
        "michael/laravelpagseguro": "dev-master",
        "namshi/jose": "^7.2",
        "nesbot/carbon": "~1.21",
        "owen-it/laravel-auditing": "^6.0",
        "tymon/jwt-auth": "1.0.0-rc.2",
        "yajra/laravel-datatables-oracle": "^8.7",
        "zircote/swagger-php": "^2.0",
        "zizaco/confide": "dev-master",
        "zizaco/entrust": "dev-master"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
